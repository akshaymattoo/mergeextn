{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Merge Windows",
  "version": "2.0",
  "description": "Merge open windows with a click",
  "icons": {
    "128": "128.png"
  },
  "action": {
    "default_icon": {
      "128": "128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "activeTab", "windows"]
}
